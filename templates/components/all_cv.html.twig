<article class="row g-4">
    {% if this.allCv is empty %}
        <div class="alert alert-warning" role="alert">
            Aucun CV n'a été trouvé.
        </div>

    {% else %}
        {% for cv in this.allCv %}
            {{ component('cv_card', {'id': cv.id}) }}
        {% endfor %}
    {% endif %}
</article>